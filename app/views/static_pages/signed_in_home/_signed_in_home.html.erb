<div class="row home-page">  
  <div id="quizzes" class="col-md-4 col-xs-6">  
    <%= render 'static_pages/signed_in_home/signed_in_home_quizzes' %>
    <div class="col-md-offset-3"> 
      <%= link_to "New Quiz", new_quiz_path, class: "btn btn-primary btn-large" %>
    </div>
  </div>  

  <div id="quiz_events" class="col-md-4 col-xs-6">  
