<%= simple_form_for(@quiz) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :author_id %>
    <%= f.input :published %>
    <%= f.association :category %>
    <%= f.association :subject %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
