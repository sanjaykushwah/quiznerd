<table class="table table-bordered">
  <tr>
    <td class="quiz_event_section">
      <%= @presenter.grade_header %></td>
  </tr>
</table>

<div class="table-responsive">
  <table class="table table-bordered graded-question">
    <tr>
      <td colspan="2">
        <%= markdown(@presenter.graded_question.content) %>
      </td>
    </tr> 
    <% @presenter.graded_question.answers.each do |answer| %>
    <tr>
      <td class="grade_icon_cell"><%= @presenter.grade_icon_for(answer) %></td>
      <td><%= answer.content %></td>
    </tr> 
    <% end %>
  </table>
</div>

<% if @presenter.remarks? %>
  <%= render 'remarks' %>
<% end %>




